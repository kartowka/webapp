{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@styles/*": [
        "src/assets/styles/*"
      ],
      "@images/*": [
        "src/assets/images/*"
      ],
      "@model/*": [
        "src/model/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
    }
  },
}